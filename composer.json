{
  "name": "cachan/bbst",
  "description": "Black box system testing",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Christopher Chan",
      "email": "cac2161@columbia.edu"
    }
  ],

  "require": {
    "monolog/monolog": "1.0.*",
    "symfony/console": "2.6.*",
    "symfony/process": "2.6.*",
    "guzzle/guzzle": "3.9.*",
    "phpseclib/phpseclib": "dev-master",
    "behat/behat": "3.0.*",
    "codeception/codeception": "2.0.*",
    "jvehent/cipherscan": "dev-master"
  },

  "config": {
    "bin-dir": "bin/"
  },

  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },

  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "jvehent/cipherscan",
        "version": "dev-master",
        "source": {
          "url": "https://github.com/jvehent/cipherscan.git",
          "type": "git",
          "reference": "origin/master"
        }
      }
    }
  ]
}